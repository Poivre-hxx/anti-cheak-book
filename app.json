{
  "expo": {
    "name": "反诈宝典",
    "slug": "anti-cheak-book",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./src/assets/icons/appIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/icons/appIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.po1vre.anticheakbook"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icons/appIcon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.po1vre.anticheakbook"
    },
    "web": {
      "favicon": "./src/assets/icons/appIcon.png"
    }
  }
}
